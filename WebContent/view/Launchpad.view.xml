<core:View
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:u="sap.ui.unified"
  xmlns="sap.m"
  controllerName="fmg.conmon.controllers.Launchpad"
  xmlns:html="http://www.w3.org/1999/xhtml">

  <u:Shell
    id="myShell"
    icon="./images/fortescuelogo.svg"
    class="sapUiMediumMarginTop launchpadShell">
    <u:headItems>
    </u:headItems>


    <u:user>
      <u:ShellHeadUserItem image="sap-icon://person-placeholder"
        username="{employeeModel>/firstName} {employeeModel>/lastName}"/>
    </u:user>

    <u:headEndItems>
      <u:ShellHeadItem
        icon="sap-icon://log"
        tooltip="Logoff"
        press="handleLogoffPress"/>
    </u:headEndItems>
    

    <u:content>
      <ScrollContainer id="tileContains"
        height="100%"
        width="100%"
        class="launchpadBackground"
        horizontal="true"
        vertical="true"
        focusable="true">
        <VBox >
          <Text
            text="FUNCTION  - 功能维护"
            class="sapUiStrongBackgroundTextColor sapUshellContainerTitle sapMH3FontSize sapUiSmallMarginTop"></Text>
          <FlexBox class="sapUiTinyMarginTop">
            <GenericTile
              header="Event List"
              id="eventlist"
              subheader=""
              size="M"
              frameType="OneByOne"
              class="tileMarginLeft"
              press="pressTile">
              <tileContent>
                <TileContent footer="Entry Point" size="M">
                </TileContent>
              </tileContent>
            </GenericTile>

           <GenericTile
              header="Watch List"
              id="watchlist"
              subheader=""
              size="M"
              frameType="OneByOne"
              class="tileMarginLeft"
              press="pressTile">
              <tileContent>
                <TileContent footer="Entry Point" size="M">
                </TileContent>
              </tileContent>
            </GenericTile>

          </FlexBox>
        </VBox>
      </ScrollContainer>

    </u:content>
  </u:Shell>
</core:View>